{
    "name": "reactor/files",
    "description": "Easy Media management for Laravel 5.6",
    "license": "MIT",
    "keywords": [
        "reactor",
        "files",
        "media",
        "management",
        "laravel",
        "laravel5"
    ],
    "version": "1.0.0",
    "authors": [
        {
            "name": "Matrix Infoline",
            "email": "infolinematrix@gmail.com.com",
            "homepage": "http://scriptbazar.com",
            "role": "Developer / Designer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.*",
        "laracasts/presenter": "0.2.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Reactor\\Files\\": "src/"
        }

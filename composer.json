    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "laravel/framework": "This package requires Laravel 5.6",
        "reactor/transit": "Easy file upload and download for Laravel 5.6",
        "kenarkose/ownable": "Allows ownership for Eloquent models on Laravel 5.6"
    }
}
